<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:background="@drawable/home_background"
             tools:context=".HomeFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="@string/current_goal"
                android:textColor="@color/white"
                android:textSize="34sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_goal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="188dp" app:layout_constraintEnd_toEndOf="parent"/>
        <ListView
                android:layout_width="358dp"
                android:layout_height="152dp"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/listView_transaction" app:layout_constraintTop_toBottomOf="@+id/textView_viewDate"
                android:layout_marginTop="8dp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="9/7/2024"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_viewDate"
                tools:layout_editor_absoluteY="473dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"
                android:textSize="20sp"/>
        <TextView
                android:text="$100,000"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_goalNumber"
                app:layout_constraintTop_toBottomOf="@+id/textView_goal"
                android:layout_marginTop="8dp" app:layout_constraintStart_toStartOf="parent"
                android:textSize="32sp" app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Monthly Balance"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_monthlyBalance"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
                app:layout_constraintTop_toBottomOf="@+id/textView_goalNumber" android:layout_marginTop="32dp"
                android:textSize="20sp"/>
        <TextView
                android:text="$700"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_monthlyBalanceNumber"
                app:layout_constraintStart_toEndOf="@+id/textView_monthlyBalance" android:layout_marginStart="148dp"
                app:layout_constraintTop_toBottomOf="@+id/textView_goalNumber" android:layout_marginTop="36dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.26"/>
        <TextView
                android:text="Income"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_monthlyIncome"
                app:layout_constraintTop_toBottomOf="@+id/textView_monthlyBalance" android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="@+id/textView_monthlyBalance"/>
        <TextView
                android:text="1,200"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_monthlyIncomeNumber"
                app:layout_constraintTop_toBottomOf="@+id/textView_monthlyBalanceNumber" android:layout_marginTop="28dp"
                app:layout_constraintStart_toEndOf="@+id/textView_monthlyIncome"
                app:layout_constraintEnd_toEndOf="@+id/textView_monthlyBalanceNumber"
                app:layout_constraintHorizontal_bias="1.0"/>
        <TextView
                android:text="Expenses"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView_Expenses"
                app:layout_constraintTop_toBottomOf="@+id/textView_monthlyIncome" android:layout_marginTop="16dp"
                app:layout_constraintStart_toStartOf="@+id/textView_monthlyIncome"/>
        <TextView
                android:text="500"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView2"
                app:layout_constraintTop_toBottomOf="@+id/textView_monthlyIncomeNumber" android:layout_marginTop="16dp"
                app:layout_constraintStart_toEndOf="@+id/textView_Expenses"
                app:layout_constraintEnd_toEndOf="@+id/textView_monthlyIncomeNumber"
                app:layout_constraintHorizontal_bias="1.0"/>
        <TextView
                android:text="TextView"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView3" tools:layout_editor_absoluteY="481dp"
                app:layout_constraintStart_toEndOf="@+id/textView_viewDate" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.85"/>
        <ImageView
                android:src="@drawable/rocket_fly_icon"
                android:layout_width="42dp"
                android:layout_height="100dp" android:id="@+id/imageView"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="104dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="44dp" android:rotation="90"/>
        <ImageView
                android:src="@drawable/moon_icon"
                android:layout_width="208dp"
                android:layout_height="261dp" android:id="@+id/imageView2"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="300dp" android:layout_marginBottom="496dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>